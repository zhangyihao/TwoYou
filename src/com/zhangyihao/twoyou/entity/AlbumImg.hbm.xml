<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zhangyihao.twoyou.entity.AlbumImg" table="album_img">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="identity"></generator>
        </id>
        <many-to-one name="images" class="com.zhangyihao.twoyou.entity.Images" fetch="select">
            <column name="imgid" not-null="true" />
        </many-to-one>
        <many-to-one name="album" class="com.zhangyihao.twoyou.entity.Album" fetch="select" lazy="false">
            <column name="albumid" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
